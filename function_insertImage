function insertImageTopLeft() {
    // Create a new img element
    let img = document.createElement('img');

    // Image source (Base64 or URL)
    img.src = "data:image/png;base64,"; // Replace with your image source

    // Set the size of the image (optional)
    img.style.width = '100px'; // Width
    img.style.height = '100px'; // Height

    // Position the image at the top-left corner
    img.style.position = 'absolute'; // Use absolute positioning
    img.style.top = '200'; // Set to top-left corner
    img.style.left = '50'; // Set to top-left corner

    document.body.appendChild(img);
